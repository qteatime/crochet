% crochet

open crochet.concurrency;
open crochet.ui.agata;
open crochet.time;

singleton todo-app;

command main-html: Root do
  timer with-real-timer: {
    agata-dom with-root: Root do: {
      let Todo = new todo([]);
      let Cell = #observable-cell with-value: Todo;
      agata show: Cell;
    };
  }
end
