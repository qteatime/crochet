% crochet

open crochet.concurrency;
open crochet.ui.agata;

singleton todo-app;

command main-html: Root do
  agata-dom with-root: Root do: {
    agata show: todo-app example-list;
  };
end

command todo-app example-list do
  new todo([
    new todo-item(new todo-id, "A", false),
    new todo-item(new todo-id, "B", false),
    new todo-item(new todo-id, "C", false),
  ])
end