% crochet

relation X* at: Y;

scene main {
  let Point = [X -> 1, Y -> 2];
  [Point.X, Point.Y] show;

  fact "player" at: "foyer";
  fact "npc" at: "foyer";
  let Location = search Who at: Where;
  Location.Who show;
  Location.Where show;

  Location.(Who as Quem, Where as Aonde) show;
}