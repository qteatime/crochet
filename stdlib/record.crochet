% crochet

command (X is record) ++ (Y is record) =
  foreign crochet.native.record.merge(X, Y);

command (X is record) keys =
  foreign crochet.native.record.keys(X);

command (X is record) values =
  foreign crochet.native.record.values(X);

command (X is record) count =
  foreign crochet.native.record.count(X);