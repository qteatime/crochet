% crochet

command #colour red: (R is integer)
                green: (G is integer)
                blue: (B is integer)
                alpha: (A is integer)
requires
  byte-components :: ((R >= 0) and (R <= 255)) and
                     ((G >= 0) and (G <= 255)) and
                     ((B >= 0) and (B <= 255))
do
  new colour(R, G, B, A);
end

